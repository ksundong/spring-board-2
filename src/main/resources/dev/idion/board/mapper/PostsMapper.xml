<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
         "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dev.idion.board.mapper.PostsMapper">
  <select id="selectPostList" resultType="HashMap">
      SELECT *
        FROM posts
       WHERE 1 = 1
